     2 00:CECD: 7F EE         	STA		P_Temp								; 将十进制输入保存到 P_Temp
     3 00:CECF: 53 00         	LDA		#0									; 初始化高位寄存器
     4 00:CED1: 7F EF         	STA		P_Temp+1							; 高位清零
     5 00:CED3: 7F F0         	STA		P_Temp+2							; 低位清零
     6                        
     7                        L_DecToHex_Loop:
     8 00:CED5: 73 EE         	LDA		P_Temp								; 读取当前十进制值
     9 00:CED7: 56 0A         	CMP		#10
    10 00:CED9: 10 09         	BCC		L_DecToHex_End						; 如果小于16，则跳到结束
    11                        
    12 00:CEDB: 00            	SEC											; 启用借位
    13 00:CEDC: 55 0A         	SBC		#10									; 减去16
    14 00:CEDE: 7F EE         	STA		P_Temp								; 更新十进制值
    15 00:CEE0: 78 EF         	INC		P_Temp+1							; 高位+1，累加十六进制的十位
    16                        
    17 00:CEE2: 17 F1         	BRA		L_DecToHex_Loop						; 重复循环
    18                        
    19                        L_DecToHex_End:
    20 00:CEE4: 73 EE         	LDA		P_Temp								; 最后剩余的值是低位
    21 00:CEE6: 7F F0         	STA		P_Temp+2							; 存入低位
    22                        
    23 00:CEE8: 73 EF         	LDA		P_Temp+1							; 将高位放入A寄存器准备结果组合
    24 00:CEEA: 04            	CLC
